<% if flash[:errors] %>
  <% flash[:errors].each do |error| %>
  <%=  error %>
  <% end %>
<% end %>


<%= form_for @duck do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
<br>
<br>
  <%= f.label :description %>
  <%= f.text_field :description %>
<br>
  <%= f.label :student_id %>
  <%= f.collection_select :student_id, Student.all, :id, :name, {include_blank: 'Select a owner'}%>
<br>

  <%= f.submit %>
<% end %>